<h1>Photos#index</h1>
<p>Find me in app/views/photos/index.html.erb</p>
<% @photos.each do |photo| %>
  <div class="card">
    <%= image_tag photo.url, class:"card-img-top", alt:"Card image" %>
      <div class="card-block">
        <h4 class="card-title">
          <%= photo.username %>
        </h4>
        <p class="card-text"><%= photo.caption %></p>
      </div>
  </div>
<% end %>
